<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="og_question" pageWidth="182" pageHeight="840" whenNoDataType="NoDataSection" columnWidth="182" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.4641000000000006"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="dejavu" isDefault="true" fontName="DejaVu Sans" fontSize="10" pdfFontName="DejaVuSans.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
	<style name="dejavu_bold" isDefault="true" fontName="DejaVu Sans" fontSize="10" isBold="true" pdfFontName="DejaVuSans-Bold.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
	<style name="table_header" style="dejavu_bold" backcolor="#CCFFFF">
		<box>
			<pen lineWidth="0.5" lineStyle="Solid" lineColor="#000000"/>
		</box>
	</style>
	<style name="text" style="dejavu">
		<box>
			<pen lineWidth="0.0" lineStyle="Solid"/>
		</box>
	</style>
	<style name="text_bold" style="table_header"/>
	<style name="table" style="dejavu">
		<box>
			<pen lineWidth="0.5" lineStyle="Solid" lineColor="#000000"/>
		</box>
	</style>
	<parameter name="card_id" class="java.lang.Long"/>
	<parameter name="attr" class="java.lang.String"/>
	<parameter name="path" class="java.lang.String" isForPrompting="false"/>
	<parameter name="tz_hour" class="java.lang.Long">
		<defaultValueExpression><![CDATA[new java.lang.Long(java.util.Calendar.getInstance().getTimeZone().getRawOffset() / 3600000)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select array_to_string(array(select
	case
        when
            av_que1.number_value is null then
                av_que_name.string_value
        else
            av_que1_name.string_value
    end as nsdr
from
	card c
left join -- Вопрос
	attribute_value av_que on (av_que.card_id = c.card_id and av_que.attribute_code = $P{attr})
    left join
        attribute_value av_que_name on (av_que_name.card_id = av_que.number_value and av_que_name.attribute_code = 'NAME')
    left join
        attribute_value av_que1 on (av_que1.card_id = av_que.number_value and av_que1.attribute_code = 'JBR_QUESTION')
        left join
            attribute_value av_que1_name on (av_que1_name.card_id = av_que1.number_value and av_que1_name.attribute_code = 'NAME')

where c.card_id = $P{card_id}), ', ' || chr(13)) as nsdr]]>
	</queryString>
	<field name="nsdr" class="java.lang.String"/>
	<detail>
		<band height="22" splitType="Stretch">
			<textField isStretchWithOverflow="true" isBlankWhenNull="false">
				<reportElement style="table" positionType="Float" stretchType="RelativeToBandHeight" isPrintRepeatedValues="false" x="0" y="0" width="182" height="22"/>
				<textElement verticalAlignment="Middle">
					<font fontName="DejaVu Sans" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA[($F{nsdr} != null ? $F{nsdr} : "")]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
