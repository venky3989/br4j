	<parameter name="from_date" class="java.util.Date"/>
	<parameter name="to_date" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="journal_reg" class="java.lang.String"/>
	<parameter name="jregs" class="java.lang.String"/>
	<parameter name="source_type" class="java.lang.String"/>
	<parameter name="divisions" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
--c.card_id,
dep.string_value as district, --источник поступления
pname.string_value as pername,
count(distinct resol.card_id) as all_resol, ---поступило всего вопросов
count(distinct control.card_id) as control, ---на контроле
count(distinct nocontrol.card_id) as nocontrol ---не на контроле
FROM card c
---проверяем дату регистрации
JOIN attribute_value reg_date ON reg_date.card_id = c.card_id and reg_date.attribute_code='JBR_REGD_DATEREG' and
	date_trunc('day', reg_date.date_value) BETWEEN  $P{from_date} and $P{to_date}
--Журнал регистрации
JOIN attribute_value add_jo ON add_jo.card_id = c.card_id and add_jo.attribute_code='JBR_REGD_REGJOURN'
--Список резолюций
JOIN attribute_value resol ON resol.number_value = c.card_id and resol.attribute_code='JBR_MAINDOC'
-- карточки поручений
left join card res_card
	on res_card.card_id = resol.card_id
--Исполнители/Соисполнители
JOIN attribute_value executors ON executors.card_id = resol.card_id and executors.attribute_code in ('JBR_INFD_EXEC_LINK', 'ADMIN_255974' )
join person p on p.person_id=executors.number_value
join attribute_value pname on pname.card_id=p.card_id and pname.attribute_code in ('NAME')
--Подразделение(Департамент)
join attribute_value pav on pav.card_id=p.card_id and pav.attribute_code in ('JBR_PERS_DEPT_LINK')
join attribute_value dep on dep.card_id=pav.number_value and dep.attribute_code in ('NAME')
---на контроле
LEFT JOIN attribute_value control ON control.card_id = resol.card_id and control.attribute_code='JBR_TCON_ONCONT' and control.value_id=1449
---не на контроле
LEFT JOIN attribute_value nocontrol ON nocontrol.card_id = resol.card_id and nocontrol.attribute_code='JBR_TCON_ONCONT' and nocontrol.value_id not in (1449)

--Контролер по документу
JOIN (SELECT DISTINCT c.card_id FROM card c left join attribute_value controller on controller.card_id=c.card_id
and controller.attribute_code='JBR_IMPL_INSPECTOR'
left join person p on p.person_id=controller.number_value
WHERE c.template_id=864 and ($P{jregs} = '-1' or p.card_id in ($P!{jregs}))
) as controller2 ON
controller2.card_id = c.card_id

--Тип контроля
left JOIN attribute_value typecontrol ON typecontrol.card_id = c.card_id
and typecontrol.attribute_code='JBR_IMPL_TYPECONT'

WHERE c.template_id=864 and
c.status_id not in (1,301,302,303990) and
($P{journal_reg} is null or add_jo.number_value in ($P!{journal_reg})) and
($P{source_type} = '-1' or typecontrol.value_id in ($P!{source_type})) and
($P{divisions} = '-1' or pav.number_value in ($P!{divisions}))
and res_card.status_id not in (1,34145,303990,10001050,107)
group by dep.string_value, district, pername
ORDER BY dep.string_value]]>
