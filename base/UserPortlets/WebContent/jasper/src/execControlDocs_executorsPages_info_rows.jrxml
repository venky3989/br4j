				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{reserve}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement style="table" x="541" y="0" width="261" height="20"/>
				<textElement/>
