<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="controlOfResolutionExecutionReport " pageWidth="143" pageHeight="18" orientation="Landscape" columnWidth="143" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="card_id" class="java.lang.Integer"/>
	<parameter name="responsibleExecutor" class="java.lang.Boolean"/>
	<queryString>
		<![CDATA[SELECT distinct
av_ext_exec_name.string_value as executor
From attribute_value c

JOIN attribute_value av_ext_exec_name  on c.number_value = av_ext_exec_name.card_id and  av_ext_exec_name.attribute_code = 'NAME'
WHERE
c.card_id=$P{card_id} and
c.attribute_code = 'JBR_INFD_EXEC_EXT']]>
	</queryString>
	<field name="executor" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<variable name="number" class="java.lang.Integer" incrementType="Column" calculation="Count"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<pageHeader>
		<band splitType="Stretch"/>
	</pageHeader>
	<detail>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" stretchType="RelativeToTallestObject" mode="Transparent" x="0" y="0" width="143" height="18" forecolor="#000000" backcolor="#FFFFFF"/>
				<box topPadding="0" leftPadding="0" bottomPadding="0" rightPadding="0">
					<pen lineColor="#FFFFFF"/>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#FFFFFF"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#FFFFFF"/>
					<bottomPen lineWidth="0.0" lineStyle="Solid" lineColor="#FFFFFF"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#FFFFFF"/>
				</box>
				<textElement textAlignment="Center" verticalAlignment="Top" rotation="None" markup="none">
					<font fontName="Times New Roman" size="10" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false" pdfFontName="timesbd.ttf" pdfEncoding="Cp1251" isPdfEmbedded="false"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{executor}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
	<columnFooter>
		<band splitType="Stretch"/>
	</columnFooter>
	<pageFooter>
		<band splitType="Stretch"/>
	</pageFooter>
	<summary>
		<band splitType="Stretch"/>
	</summary>
</jasperReport>
