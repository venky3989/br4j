<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="values_list_instr_exe" printOrder="Horizontal" pageWidth="555" pageHeight="842" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<style name="dejavu" isDefault="true" fontName="DejaVu Sans" fontSize="10" pdfFontName="DejaVuSans.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
	<style name="text" isDefault="false" style="dejavu">
		<box>
			<pen lineWidth="0.0" lineStyle="Solid"/>
		</box>
	</style>
	<parameter name="card_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[
select	case
		when vl.value_id <> 0
		then vl.value_rus
		else av.another_value
	end val
from	attribute_value av, values_list vl
where 	av.card_id = $P{card_id} and
	av.attribute_code = 'ADMIN_282186' and
	vl.value_id = av.value_id
]]>
	</queryString>
	<field name="val" class="java.lang.String"/>
	<detail>
		<band height="15" splitType="Stretch">
			<textField>
				<reportElement style="text" x="0" y="0" width="555" height="15"/>
				<textElement textAlignment="Left" verticalAlignment="Middle"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{val} != null ? $F{val} : ""]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
