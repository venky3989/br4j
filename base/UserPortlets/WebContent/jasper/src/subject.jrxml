<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subject" printOrder="Horizontal" pageWidth="555" pageHeight="842" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<style name="dejavu" isDefault="true" fontName="DejaVu Sans" fontSize="10" pdfFontName="DejaVuSans.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
	<style name="text" isDefault="false" style="dejavu">
		<box>
			<pen lineWidth="0.0" lineStyle="Solid"/>
		</box>
	</style>
	<parameter name="card_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[
select	av_name.string_value as name
from	attribute_value av_subject
left outer join
	attribute_value av_name on (av_name.card_id = av_subject.number_value and av_name.attribute_code = 'NAME')
where	av_subject.card_id = $P{card_id} and av_subject.attribute_code = 'ADMIN_277249'
]]>
	</queryString>
	<field name="name" class="java.lang.String"/>
	<detail>
		<band height="15" splitType="Stretch">
			<textField>
				<reportElement style="text" x="0" y="0" width="555" height="15"/>
				<textElement textAlignment="Left" verticalAlignment="Middle"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{name} != null ? $F{name} : ""]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>