<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="cardChangesValueCards" pageWidth="126" pageHeight="842" columnWidth="126" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="dejavu" isDefault="true" fontName="DejaVu Sans" fontSize="10" pdfFontName="DejaVuSans.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
	<style name="dejavu_bold" isDefault="true" fontName="DejaVu Sans" fontSize="13" isBold="true" pdfFontName="DejaVuSans-Bold.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
	<style name="text" isDefault="false" style="dejavu">
		<box>
			<pen lineWidth="0.0" lineStyle="Solid"/>
		</box>
	</style>
	<style name="text_bold" isDefault="false" style="dejavu_bold"/>
	<style name="table" isDefault="false" style="dejavu">
		<box>
			<pen lineWidth="0.5" lineStyle="Solid" lineColor="#000000"/>
		</box>
	</style>
	<parameter name="cardId" class="java.lang.Long"/>
	<parameter name="versionId" class="java.lang.Long"/>
	<parameter name="attr_code" class="java.lang.String"/>
	<parameter name="attr_type" class="java.lang.String"/>
	<queryString>
		<![CDATA[select av_name.string_value as card_name
from (
        select  avh.number_value as card_id
        from 	attribute_value_hist avh
        where	avh.card_id = $P{cardId} and avh.version_id = $P{versionId} 
		and avh.attribute_code = $P{attr_code}
) as av_card
inner join
	attribute_value av_name on (av_name.attribute_code = 'NAME' and av_name.card_id = av_card.card_id)]]>
	</queryString>
	<field name="card_name" class="java.lang.String"/>
	<detail>
		<band height="15" splitType="Stretch">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement style="text" stretchType="RelativeToTallestObject" x="0" y="0" width="126" height="15"/>
				<box leftPadding="5"/>
				<textElement textAlignment="Left" verticalAlignment="Top">
					<font fontName="Times New Roman" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{card_name} != null ? $F{card_name} : ""]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
