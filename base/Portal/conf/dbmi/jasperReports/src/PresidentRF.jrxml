	<queryString>
		<![CDATA[Select
    c.card_id as card_id,
    av_date_doc.date_value + interval '$P!{tz_hour} hour' as date_doc,
    av_number.string_value as number_doc,
    av_created.date_value + interval '$P!{tz_hour} hour' as created,
    av_reg_date.date_value + interval '$P!{tz_hour} hour' as reg_date,
    case when (
               c.status_id in (101,102,103)
              )
        then 1
        else null
    end as on_cons
    from card c
    left join attribute_value av_date_doc on c.card_id = av_date_doc.card_id and av_date_doc.attribute_code = 'JBR_REGD_DATEOUT'
    join attribute_value av_source on c.card_id = av_source.card_id and av_source.attribute_code = 'ADMIN_281718'
    left join attribute_value av_number on c.card_id = av_number.card_id and av_number.attribute_code = 'JBR_REGD_NUMOUT'
    left join attribute_value av_created on c.card_id = av_created.card_id and av_created.attribute_code = 'CREATED'
    left join attribute_value av_reg_date on c.card_id = av_reg_date.card_id and av_reg_date.attribute_code = 'JBR_REGD_DATEREG'
    where c.template_id = 864
    and c.status_id not in (303990,1,302,301)
    and (date_trunc('day', av_date_doc.date_value + interval '$P!{tz_hour} hour') <= $P{endDate}
    and date_trunc('day', av_date_doc.date_value + interval '$P!{tz_hour} hour') >= $P{beginDate} and $P{searchWith} = 'kitQuarter'
	or
    date_trunc('day', av_reg_date.date_value + interval '$P!{tz_hour} hour') <= $P{endDate}
    and date_trunc('day', av_reg_date.date_value + interval '$P!{tz_hour} hour') >= $P{beginDate} and $P{searchWith} = 'kitPeriod'
)
    and av_source.value_id = 3387
    order by date_doc, number_doc]]>
	</queryString>
